# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# RPC URLs (optional - can use foundry.toml instead)
LOCAL_RPC_URL=http://127.0.0.1:8545
UNICHAIN_RPC_URL=https://130.rpc.thirdweb.com
UNICHAIN_SEPOLIA_RPC_URL=https://1301.rpc.thirdweb.com

# Additional network URLs (if needed)
ALCHEMY_API_KEY=your_alchemy_key_here
INFURA_API_KEY=your_infura_key_here

# Development settings
FOUNDRY_PROFILE=default

# Gas price settings (in gwei)
LOCAL_GAS_PRICE=1
UNICHAIN_GAS_PRICE=0.1
UNICHAIN_SEPOLIA_GAS_PRICE=0.1